{
  "name": "inflowave",
  "version": "0.1.2",
  "description": "A modern time-series database management tool built with Tauri + React + TypeScript",
  "type": "module",
  "scripts": {
    "dev": "npm run copy-docs && vite",
    "build": "npm run copy-docs && vite build",
    "copy-docs": "node scripts/copy-user-docs.js",
    "build-check": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "node scripts/start-with-port-check.cjs --dev",
    "tauri:dev:direct": "npm run copy-docs && tauri dev",
    "tauri:dev:safe": "node scripts/start-with-port-check.cjs --dev",
    "tauri:dev:server": "ENABLE_EMBEDDED_SERVER=true npm run tauri:dev:safe",
    "tauri:build": "npm run copy-docs && tauri build",
    "tauri:build:server": "ENABLE_EMBEDDED_SERVER=true npm run tauri:build",
    "tauri:build:release": "tauri build --release",
    "build:windows-x64": "tauri build --target x86_64-pc-windows-msvc",
    "build:windows-x86": "tauri build --target i686-pc-windows-msvc",
    "build:windows-arm64": "echo 'Windows ARM64 暂时禁用，等待 ring 库支持改善'",
    "build:windows": "npm run build:windows-x64 && npm run build:windows-x86",
    "build:macos-x64": "tauri build --target x86_64-apple-darwin",
    "build:macos-arm64": "tauri build --target aarch64-apple-darwin",
    "build:macos": "npm run build:macos-x64 && npm run build:macos-arm64",
    "build:linux-x64": "tauri build --target x86_64-unknown-linux-gnu",
    "build:linux-arm64": "tauri build --target aarch64-unknown-linux-gnu",
    "build:linux-x86": "tauri build --target i686-unknown-linux-gnu",
    "build:linux": "npm run build:linux-x64 && npm run build:linux-arm64 && npm run build:linux-x86",
    "build:all": "npm run build:windows && npm run build:macos && npm run build:linux",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:src": "eslint src/ --report-unused-disable-directives",
    "lint:fix": "eslint . --fix",
    "lint:fix-all": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,html}\" --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "code-quality": "npm run type-check && npm run lint:fix-all && npm run format:all",
    "fix-all": "node scripts/fix-code-quality.js",
    "clean-imports": "eslint . --ext ts,tsx --fix",
    "check-unused": "eslint . --ext ts,tsx",
    "prepare": "node scripts/prepare.js",
    "version:check": "node scripts/sync-version.cjs check",
    "version:sync": "node scripts/sync-version.cjs sync",
    "version:bump": "node scripts/sync-version.cjs bump",
    "version:bump:patch": "node scripts/sync-version.cjs bump patch --tag",
    "version:bump:minor": "node scripts/sync-version.cjs bump minor --tag",
    "version:bump:major": "node scripts/sync-version.cjs bump major --tag",
    "clean": "npm run clean:cache && npm run clean:build && npm run clean:deps",
    "clean:cache": "rm -rf node_modules/.cache && rm -rf node_modules/.vite && rm -rf .vite && rm -rf dist/.vite",
    "clean:build": "rm -rf dist && rm -rf src-tauri/target",
    "clean:deps": "rm -rf node_modules && npm install",
    "clean:all": "npm run clean:build && npm run clean:deps && npm run clean:tauri",
    "clean:tauri": "cd src-tauri && cargo clean && cd .. && rm -rf src-tauri/target",
    "reset": "npm run clean:all && npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@influxdata/influxdb-client": "^1.35.0",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.3.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "glob": "^11.0.3",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.525.0",
    "minimatch": "^10.0.3",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^3.0.3",
    "react-router-dom": "^6.20.1",
    "recharts": "2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^4.0.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.13",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-grid-layout": "^1.3.5",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "inflowave",
    "time-series",
    "database",
    "gui",
    "tauri",
    "react",
    "typescript",
    "influxdb",
    "visualization",
    "analytics"
  ],
  "author": "Kkape",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenqi92/inflowave.git"
  },
  "bugs": {
    "url": "https://github.com/chenqi92/inflowave/issues"
  },
  "homepage": "https://allbs.cn"
}
